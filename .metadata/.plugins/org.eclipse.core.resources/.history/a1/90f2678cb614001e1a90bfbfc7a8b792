package sample;

public class TestBirdSighting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BirdSighting sighting1 = new BirdSighting();
        System.out.println("Species: " + sighting1.getSpecies());
        System.out.println("Number seen: " + sighting1.getNumberSeen());
        System.out.println("Day of year: " + sighting1.getDayOfYear());
        
        BirdSighting sighting2 = new BirdSighting("sparrow", 3, 91);
        System.out.println("Species: " + sighting2.getSpecies());
        System.out.println("Number seen: " + sighting2.getNumberSeen());
        System.out.println("Day of year: " + sighting2.getDayOfYear());
    }
}
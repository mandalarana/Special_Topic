package common;

public class Student {

	private int uid;
	private String Fname;
	private String Lname;
	private String grade;
	private float tution;

	public int getuid() {
		return uid;
	}

	public String getFname() {
		return Fname;
	}

	public String getLname() {
		return Lname;
	}

	public float gettution() {
		return tution;
	}

	public String getgrade() {
		return grade;
	}

	public void setuid(int studentid) {
		uid = studentid;
	}

	public void setFname(String f_name) {
		Fname = f_name;
	}

	public void setLname(String l_name) {
		Lname = l_name;
	}

	public void settution(float fee) {
		tution = fee;
	}

	public void grade(String grade) {
		grade = grade;
	}
	
	public void setStudent (int studentId,String f_name, String l_name, float fee, String grade)
	{
		uid = studentId;
		Fname=f_name;
		Lname=l_name;
		grade=grade;
		tution=fee;
	}

	//public void setgrade(String nextLine) {
		// TODO Auto-generated method stub
		
	}
	


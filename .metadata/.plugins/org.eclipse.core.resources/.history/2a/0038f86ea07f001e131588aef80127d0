package SE;

public class If_condition {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] num = {1,2,4,5,6};

		for(int i = 0; i<num.length; i++)
		{
			if(num[i] %2 ==0)
			{
			System.out.println(num[i]);
			//break;
			}
			else
			{
				System.out.println(num[i] + " not mutliple of 2");
			}
		}
	}

}
